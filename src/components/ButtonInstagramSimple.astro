---
const { contentAriaL } = Astro.props;
---

<button
  aria-label=`${contentAriaL}`
  class="flex flex-row gap-1 border-2 border-white rounded-full p-2 transition-all ease-in-out hover:scale-105"
>
  <svg
    width="24"
    height="24"
    viewBox="0 0 24 24"
    fill="none"
    stroke="currentColor"
    stroke-width="2"
    stroke-linecap="round"
    stroke-linejoin="round"
    class="icon icon-tabler icons-tabler-outline icon-tabler-brand-instagram"
    ><path stroke="none" d="M0 0h24v24H0z" fill="none"></path><path
      d="M4 4m0 4a4 4 0 0 1 4 -4h8a4 4 0 0 1 4 4v8a4 4 0 0 1 -4 4h-8a4 4 0 0 1 -4 -4z"
    ></path><path d="M12 12m-3 0a3 3 0 1 0 6 0a3 3 0 1 0 -6 0"></path><path
      d="M16.5 7.5l0 .01"></path></svg
  >
</button>
